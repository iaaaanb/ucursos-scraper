# U-Cursos Authentication Credentials
# Copy this file to .env and fill in your actual credentials
# NEVER commit the .env file to version control

UCURSOS_USERNAME=your_username_here
UCURSOS_PASSWORD=your_password_here

# U-Cursos Portal URL
UCURSOS_URL=https://www.u-cursos.cl

# Download settings
DOWNLOAD_PATH=./downloads

# Browser settings
BROWSER=chromium

# Run in headless mode (true/false)
HEADLESS=true
